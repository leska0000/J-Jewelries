          <ScalarProperty Name="amount" ColumnName="amount" />
          <ScalarProperty Name="costumer_person" ColumnName="costumer_person" />
          <ScalarProperty Name="purchase_date" ColumnName="purchase_date" />
        </MappingFragment>
      </EntityTypeMapping>
    </EntitySetMapping>
  </EntityContainerMapping>
</Mapping>