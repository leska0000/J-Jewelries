    <EntitySetMapping Name="Categories">
      <EntityTypeMapping TypeName="UjewelriesModel.Category">
        <MappingFragment StoreEntitySet="Category">
          <ScalarProperty Name="Id" ColumnName="Id" />
          <ScalarProperty Name="category_name" ColumnName="category_name" />
        </MappingFragment>
      </EntityTypeMapping>
    </EntitySetMapping>
    <EntitySetMapping Name="Costumers">
      <EntityTypeMapping TypeName="UjewelriesModel.Costumer">
        <MappingFragment StoreEntitySet="Costumer">
          <ScalarProperty Name="auto_Id" ColumnName="auto_Id" />
          <ScalarProperty Name="personal_id" ColumnName="personal_id" />
          <ScalarProperty Name="name" ColumnName="name" />
          <ScalarProperty Name="login_name" ColumnName="login_name" />
          <ScalarProperty Name="password" ColumnName="password" />
          <ScalarProperty Name="is_manager" ColumnName="is_manager" />
        </MappingFragment>
      </EntityTypeMapping>
    </EntitySetMapping>
    <EntitySetMapping Name="Products">
      <EntityTypeMapping TypeName="UjewelriesModel.Product">
        <MappingFragment StoreEntitySet="Product">
          <ScalarProperty Name="Id" ColumnName="Id" />
